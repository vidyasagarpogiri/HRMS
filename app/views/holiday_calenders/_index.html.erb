<div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h2 class="page-header"> Holiday List</h2>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Holiday List Form 
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-6">
                                <% @holiday_calenders.each do |f| %>
                                    
                                        <div class="form-group">
                                            <label style="font-weight:normal;">Date of Event:</label>
                                            <%= f.date %>                                 
                                        </div>
                                        
                                        <div class="form-group">
                                            <label style="font-weight:normal;">Event Name:</label>
                                            <%= f.event %>
                                        </div>
                                        
                                        
                                        <ul class="navigation-btns" style="list-style: none;display:inline;">  
                                        <%= link_to "Edit", edit_group_holiday_calender_path(@group, f), :class=>"btn btn-success pull-right" %>                                          
                                      <!--      <li><button type="submit" class="btn btn-success pull-left">Save</button> </li> --> 
                                        </ul>
                                  <hr>
                                 
                                </div>
                                <% end %>
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>

