<div class="wrapper">
  <div class="container">
    <div class="row">                                             
      <div class="span12">
        <div class="content">
          <div class="module">
            <div class="module-head">
              <h3>Job Location</h3>
                <% if current_user.department == Department::HR %>
              <ul class="navigation-btns pull-right" style="margin-top:-20px">
                <li><%= link_to 'Edit', edit_job_location_path(@job_location.id), :class => "btn btn-success " %></li>
                <li><%#= link_to "Delete", amzur_event_path(@amzurevent), method: :delete, data: {confirm: "Are You sure?"}, :class=>"btn btn-danger " %></li>
              </ul>  
            <% end %> 
              </div>
            <div class="module-body">               
            	 <table class="table table-striped table-bordered table-condensed" id="sample-table-2">														   
					        <tbody>
					          <tr>
					            <td><strong>Lline1:</strong></td>
					            <td><%= @job_location.address.line1 %></td>
					          </tr>
					          <tr>
					            <td><strong>Line2:</strong></td>
					            <td><%= @job_location.address.line %></td>
					          </tr>
					          <tr>
					            <td><strong>City:</strong></td>
					            <td><%= @job_location.address.city %></td>
					          </tr>
					          <tr>
					            <td><strong>State:</strong></td>
					            <td><%= @job_location.address.state %></td>
					          </tr>
					          <tr>
					            <td><strong>Country:</strong></td>
					            <td><%= @job_location.address.country %></td>
					          </tr>
					          <tr>
					            <td><strong>Zipcode:</strong></td>
					            <td><%= @job_location.address.zipcode %></td>
					          </tr>
					        </tbody>
					      </table> 
            </div><!--module-body -->    																																											                               
          </div><!-- /.module-->									      				                                      
        </div><!--/.content-->                    
      </div><!--/.span12-->                
    </div><!--/.row-->
  </div><!--/.container-->
</div><!--/.wrapper-->


