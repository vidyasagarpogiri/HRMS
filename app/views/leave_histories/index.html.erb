

<div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h2 class="page-header"> Leave History Details</h2>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Leave History d
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-6">
                                    <% @leave_histories.each do |leave_history| %>
                                        
                                        <div class="form-group">
                                            <label>Leave From:</label>
                                            <%= leave_history.from_date %>
                                        </div>
                                        <div class="form-group">
                                            <label>Leave Till:</label>
                                            <%= leave_history.to_date %>
                                        </div>
                                        <div class="form-group">
                                            <label>Total no of days:</label>
                                            <%= leave_history.days %>
                                        </div>
                                         <div class="form-group">
                                            <label>Reason for leave:</label>
                                            <%= leave_history.reason %>
                                        </div>
                                        <div class="form-group">
                                            <label>Feedback from respective authority:</label>
                                            <%= leave_history.feedback %>
                                        </div>
                                        <ul class="navigation-btns" style="list-style: none;display:inline;">                                            
                                            <li><button type="submit" class="btn btn-success pull-left">Save</button> </li>  
                                        </ul>
                                  <hr>
                                   <% end %>
                                </div>
                                
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>

