
	  <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <div class="page-header">Applied Leaves</div>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            HR View
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">
                                    <table class="table table-striped table-hover" id="sample-table-2">
											                  <thead>											      
												                  <tr>
												                    <th> Full Name </th>
													                  <th> Date </th>
													                  <th> Day(s) </th>
													                  <th> Reason </th>
													                  <th> Leave Type </th>
													                  <th> Remaining Leaves </th>
													                  <th> Used Leaves </th>														
													                  <th> Status</th>
												                    <th> Comments</th>													                        										      		
												                  </tr>
											                  </thead>											                  
	                                       <% @leave_histories.each do |f| %>
											                  <tbody>
												                  <tr> 
												                    <td> <!-- <%Employee Name %> --> </td>
												                    <td> <%= format_date (f.from_date %> to <%= format_date (f.to_date )%>  </td>
												                    <td> <%= f.days %> days(s) </td>
  											                    <td> <%= f.reason %> </td>
  											                    <td> <%= f.leave_type.type_name if f.leave_type.present? %> </td>
													                  <td><!--<% ReMaining Leaves Leave %> --> </td>
													                  <td><!--<% used Leave %> --> </td>
													                  <td> <%= f.status %> </td>
													                  <td> <%= f.feedback %> </td>           													                  
													                  <td> <%= f.status %> </td>
												                  </tr>
											                  </tbody>
											                  <% end %>
									                  </table>              
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->
                        </div>
