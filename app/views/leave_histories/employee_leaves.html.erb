             <div id="page-wrapper">  
            
						<label style="font-weight:normal;padding:13px;" class = "pull-right"><%= link_to 'Apply Leave',  new_leave_history_path(), :class => "fa fa-plus " %> </label> <br>

                   <div class="panel panel-default">
                      <div class="panel-heading">
                          Total Applied Leave List
                      </div>
                    <div class="panel-body">
										    <table class="table table-striped table-hover" id="sample-table-2">
											<thead>
												<tr>
												  <th> Full Name</th>
													<th class="center">Date</th>
													<th class="hidden-xs">No 0f Days</th>													
													<th class="hidden-xs">Leaves Type</th>
													<th> Reason </th>
													<th class="hidden-xs">Leaves Balance</th>
													<th> Leaves Used </th>
													<th class="hidden-xs">Feedback</th>
													<th class="hidden-xs">Status</th>													
												</tr>
											</thead>
										 <% @leaves.each do |f| %>
												<% unless f.status == "HOLD" %>
											<tbody>												
												<tr>               
												  <td> <%= @employee.full_name %></td>
											    <td> <%= f.from_date %> to <%= f.to_date %>  </td>
												  <td> <%= f.days %> </td>
												  <td> <%= f.leave_type.type_name if f.leave_type.present? %>  </td>
												  <td> <%= f.reason %> </td> 
												  <td> 10</td>
												  <td> 4 </td>
												  <td> <%= f.feedback %> </td>
												  <td> <%= f.status %> </td>
												</tr>
											
												
												
											</tbody>
								<% end %>
	<% end %>
										</table>
									</div>
								</div>
                   
                    
                    </div>
                    <!-- /.panel .chat-panel -->
                </div>
                <!-- /.col-lg-4 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /#page-wrapper -->

