             <div id="page-wrapper">
                <div class="row">
                    <div class="col-lg-12">
                      <div class="page-header">Employees Leaves</div>
                    </div>
                <!-- /.col-lg-12 -->
                </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">              
								<% unless f.status == "HOLD" %>
                   <div class="panel panel-default">                  
                      <div class="panel-heading">
                          Total Applied Leave List
                      </div>
                    <div class="panel-body">                    
										<table class="table table-striped table-hover" id="sample-table-2">
											<thead>
												<tr>
												  <th> Full Name</th>
													<th class="center">Date</th>
													<th class="hidden-xs">No 0f Days</th>													
													<th class="hidden-xs">Leaves Type</th>
													<th> Reason </th>
													<th class="hidden-xs">Leaves Balance</th>
													<th> Leaves Used </th>
													<th class="hidden-xs">Feedback</th>
													<th class="hidden-xs">Status</th>													
												</tr>
											</thead>
										  <% @leaves.each do |f| %>
											<% unless f.status == "HOLD" %>
											<tbody>												
												<tr>               
												  <td> <%= f.employee.full_name %></td>
											    <td> <%= format_date(f.from_date) %> to <%= format_date(f.to_date) %>  </td>
												  <td> <%= f.days %> </td>
												  <td> <%= f.leave_type.type_name if f.leave_type.present? %>  </td>
												  <td> <%= f.reason %> </td> 
												  <td> 10</td>
												  <td> 4 </td>
												  <td> <%= f.feedback %> </td>
												  <td> <%= f.status %> </td>
												</tr>
                     </tbody>
								<% end %>
	              <% end %>
										</table>

								<% else %>
                 <div class="alert alert-info alert-dismissable">
                          <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                           You have No Applied Leaves
                     </div>
									
									</div>
								</div>
                   	<% end %>
							 

                    
                    </div>
                    <!-- /.panel .chat-panel -->
                </div>
                <!-- /.col-lg-4 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /#page-wrapper -->

