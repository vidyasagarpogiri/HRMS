<h1>Contact Details</h1>

<h3>Present Address </h3><hr>
<%= form_for(@address) do |f| %>
  <%= f.label :line, "Line1" %>
  <%= f.text_field :line %> <br>
  
  <%= f.label :line1, "Line2" %>
  <%= f.text_field :line1 %> <br>

<h3>Parmanent Address </h3><hr>
  <%= f.label :line3, "Line1" %>
  <%= text_field_tag :line3 %> <br>
  <%= f.label :line4, "Line2" %>
  <%= text_field_tag :line4 %> <br>
 
<%= f.submit "Submit"%>
<% end %>
