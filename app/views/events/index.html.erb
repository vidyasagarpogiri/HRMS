
              <div align="center"><h3 class="title">List Of All Events </h3></div>
                  <label style="font-weight:normal;padding:20px;" class = "pull-right"><i class="fa fa-plus "></i> <%= link_to "Add New Event", new_event_path %> </label>
                  <div class="col-lg-12">
                    <table class="table table-striped table-hover" id="sample-table-2">
                    <% @events.each do |event| %>
											<tbody>
												<tr>               
													<td class="center"><%= event.event_name %></td>
													<td><%= event.event_date %></td>
                          <td>
													    <ul class="navigation-btns" style="list-style: none;display:inline;">
													      <li> <%= link_to 'EDIT', edit_event_path(event), :class => "btn btn-success pull-left" %></li>
													      <li> <%= link_to 'DELETE', event_path(event.id), method: :delete , :class => "btn btn-danger pull-left" %></li>
													    </ul> 
													 </td>
												</tr>
											</tbody>
										<% end %> 
									  </table> 
							</div>                                  
