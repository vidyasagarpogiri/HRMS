<div class="module-head">
<h3>Salary Structure</h3>
</div>                  
<div class="module-body">
  <table class="table table-striped table-hover" id="sample-table-2">
    <thead>
      <tr>
        <th>Salary Type</th>
        <th>percentage</th>
      <tr/>
    </thead>	
    </tbody>
    <% @static_salaries.each do |salary| %>  
      <tr>
        <td><%= salary.name %></td>
        <td><%= salary.value %></td>
      <tr>
    <% end %>
    </tbody>
  </table>
</div>
