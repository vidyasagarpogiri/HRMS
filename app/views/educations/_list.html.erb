<div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
             <!--       <h2 class="page-header">Education Details</h2> -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">
                                  <table class="table table-striped table-hover" id="sample-table-2">
											            <thead>											      
												            <tr>
													            <th class="center">Degree</th>
													            <th>Specilization </th>	
													            <th>Releaved On </th>
													            <th>Aggregate/CGPA</th>	
													            <th></th>									      		
												            </tr>
											            </thead>
											             <% list.each do |education| %>
											            <tbody>
												            <tr>               
													            <td class="center"><%= education.institute %></td>
													            <td> <%= education.specilization %> </td>
													            <td> <%= education.year_of_pass %> </td>
													            <td> <%= education.cgpa_percentage %> </td>
                                      <% if current_user.employee.role.id == 2 %>
													            <td> 
													              <ul class="navigation-btns" style="list-style: none;display:inline;">
                           <li><%= link_to 'Edit', edit_employee_education_path(@employee.id, education.id), :class => "btn btn-success pull-right" %><li>
                           <li><%= link_to 'Delete', edit_employee_education_path(@employee.id, education.id), method: :delete, data: {confirm: "Are You sure?"},:class => "btn btn-danger pull-right" %><li> </ul>
                                     </td>
                                     <% end %>
												            </tr>
											            </tbody>
											            <% end %>
									            </table>          
                                  
                                   
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>

