<div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Education view Details</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
            
                <div class="col-lg-12">
                <%= link_to 'New Education', new_employee_education_path(@employee.id), :class => "btn btn-success pull-right" %>
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-6">
                                    <% @eudcations.each do |education| %>
                                     <%= link_to 'Edit', edit_employee_education_path(@employee.id, education.id), :class => "btn btn-success pull-right" %>
                                     <%= link_to 'Delete', edit_employee_education_path(@employee.id, education.id), :class => "btn btn-success pull-right" %>                                
                                        <div class="form-group">
                                            <label style="font-weight:normal;">Name Of Graduation:</label>
                                            <%= education.specilization %>                                 
                                        </div>
                                        
                                        <div class="form-group">
                                            <label style="font-weight:normal;">College/University Name:</label>
                                            <%= education.institute %>
                                        </div>
                                        
                                        <div class="form-group">
                                            <label style="font-weight:normal;">Year of passing:</label>
                                            <%= education.year_of_pass %>                                        
                                        </div>
                                        
                                        <div class="form-group">
                                            <label style="font-weight:normal;">Percentage/CGPA::</label>
                                           <%= education.cgpa_percentage %>                                            
                                        </div>
                                        <br>
                                        <ul class="navigation-btns" style="list-style: none;display:inline;">
                                          
                                        </ul>
                                  <hr>
                                   <% end %>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                                <div class="col-lg-6">
                                    <h1></h1>
                                    <form role="form">
                                    <!--    <fieldset disabled> -->
                        
                                    </form>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>

