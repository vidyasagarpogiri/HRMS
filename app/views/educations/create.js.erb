<% if @errors.present? %>
	 $(".errors").html(" ");
		  <% @errors.each do|error| %>
			 $(".errors").append("<%=j render('/shared/alert_error', message: error) %>");
			<% end %>
<% else %>
<% if @form_type == "Save" %>
 $(".education-index").html("<%=j render  'educations/list', :list => @list %>");
 $(".new-edcuation").hide(); 
<% else %>
   $(".new-edcuation").html("<%=j render  'educations/qualification_form' %>");
  $(".education-index").html("<%=j render  'educations/list', :list => @list %>");
 
<% end %>


<% end %>
