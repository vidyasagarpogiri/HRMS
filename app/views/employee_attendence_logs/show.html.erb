<div class="wrapper">
  <div class="container">
    <div class="row">                                             
      <div class="span12">
        <div class="content">
          <div class="module">
             <div class="module-head" style="background-color: #82B4E6 ">
                <h3> Login Details</h3>
              </div>                                                                                        
              <div class="module-body">                                                                          								
                <table class="table table-striped table-bordered table-condensed">
								  <thead>
									  <tr>									    
									    <th>Time</th>									     
                      <th></th>
                      <th></th>                          
									  </tr>
								  </thead>
								  <tbody>
									<% @.each do |inOut| %>
                    <tr>
                      <td><%= inOut.time %></td> 
                      <td><%= inOut.date %></td>                    
                      <td><%= inOut.is_present %></td>
                      <td><%= inOut.total_working_hours %></td>
                      <td><% link_to 'view', :class="btn btn-success" %></td>                    
                     </tr>
                  <% end %>
								  </tbody>
								</table>			                  
              </div><!-- module body-->
            </div><!--/.module-->
          </div><!--/.content-->                    
        </div><!--/.span9-->                
     </div><!--/.row-->
  </div><!--/.container-->
</div><!--/.wrapper-->

