<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="span12">
        <div class="content">
          <div class="module">
            <div class="module-head">
					    <h3>Comapny Details</h3>
					    
					  </div>
			      <div class="module-body">							
			      <table class="table table-striped table-bordered table-condensed">
               <thead>
               <tr>
                  <th>Month</th>
                  <th>Year</th>
                  <th>Status</th>
                  <th>Name</th>
                  <th>Date & Time</th>                  
                </tr>
               </thead>               
               <tbody>
               <% @pay_rolls.each do |cprm| %>
                <tr>                  
                  <td><%= cprm.month %></td>
                  <td><%= cprm.year %></td>
                  <td><%= cprm.status %></td>
                  <td><%= cprm.name %></td>
                  <td><%= cprm.created_at %></td>
                </tr>
                <% end %>
               </tbody>                 
              </table>
						</div><!--/.module-body-->
					</div><!--/.module-->
				</div><!--/.content-->
			</div><!--/.span12-->
		</div><!--/.row-->
	</div><!--/.container-->
</div><!--/.wrapper-->
	
