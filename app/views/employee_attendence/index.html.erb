<% debug(@employeeattendece) %>

<div class="wrapper">
  <div class="container">
    <div class="row">                                             
      <div class="span12">
        <div class="content">
          <div class="module">
             <div class="module-head" style="background-color: #82B4E6 ">
                <h3> Login Details</h3>
              </div>                                                                                        
              <div class="module-body">                                                                          								
                <table class="table table-striped table-bordered table-condensed">
								  <thead>
									  <tr>									    
									    <th>Employee</th>									     
                      <th>LogDate</th>
                      <th>Is Present</th>
                      <th>Worked Hours</th>
                      <th>View History</th>                          
									  </tr>
								  </thead>
								  <tbody>
									<% @employeeattendece.each do |employeeattendece| %>
                    <tr>
                      <td><%= employeeattendece.employee_id %></td> 
                      <td><%= employeeattendece.log_date %></td>                    
                      <td><%= employeeattendece.is_present %></td>
                      <td><%= employeeattendece.total_working_hours %></td>
                      <td><%= link_to'View', :class => "btn btn-success " %></td>               
                     </tr>
                  <% end %>
								  </tbody>
								</table>			                  
              </div><!-- module body-->
            </div><!--/.module-->
          </div><!--/.content-->                    
        </div><!--/.span9-->                
     </div><!--/.row-->
  </div><!--/.container-->
</div><!--/.wrapper-->


