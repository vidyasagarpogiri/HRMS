 <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h2 class="page-header">Expereince view Details</h2>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-6">
                                   <%= form_for ([@employee, @experience]) do |f| %>
                                        
                                        <div class="form-group">
                                            <label style="font-weight:normal;">Company Name:</label>
                                            <%= f.text_field :previous_company, :class => "form-control"%>                               
                                        </div>
                                        
                                        <div class="form-group">
                                            <label style="font-weight:normal;">Designation:</label>
                                            <%= f.text_field :last_designation, :class => "form-control"%>
                                        </div>
                                        
                                        <div class="form-group">
                                            <label style="font-weight:normal;">From:</label>
                                            <%= f.text_field :from_date, :class => "form-control datepicker"%>                                        
                                        </div>
                                        
                                        <div class="form-group">
                                            <label style="font-weight:normal;">To:</label>
                                            <%= f.text_field :to_date, :class => "form-control datepicker"%>                                            
                                        </div>
                                        <br>
                                        <ul class="navigation-btns" style="list-style: none;display:inline;">
                                          <li><button type="submit" class="btn btn-success pull-left">Update</button></li>
                                        </ul>
                                        
                                   <% end %>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                                <div class="col-lg-6">
                                    <h1></h1>
                                    <form role="form">
                                    <!--    <fieldset disabled> -->
                        
                                           
              
                                    </form>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>
