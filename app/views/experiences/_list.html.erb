  <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <div class="page-header">Experience Details</div>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">
                                    <table class="table table-striped table-hover" id="sample-table-2">
											                <thead>											      
												                <tr>
													                <th class="center">Company </th>
													                <th>Designation </th>	
													                <th>Joining Date</th>
													                <th>Relieving date</th>
													                <th>working Period</th>		
													                <th></th>									      		
												                </tr>
											                </thead>
											                  <% list.each do |experience| %>
											                <tbody>
												                <tr>               
													                <td class="center"><%= experience.previous_company %></td>
													                <td> <%= experience.last_designation %> </td>
													                <td> <%= format_exp_date(experience.from_date) %> </td>
													                <td> <%= format_exp_date(experience.to_date) %> </td>
                                          <td> <%= difference_in_months(experience.from_date,experience.to_date )%> </td>
													                
                                         <% if current_user.department == Department::HR %>
													                <td> 
													                  <ul class="navigation-btns" style="list-style: none;display:inline;">
                                              <li><%= link_to 'Edit', edit_employee_experience_path(@employee.id, experience.id), :class => "btn btn-success pull-left" %></li>                        
                                              <li><%= link_to "Delete", employee_experience_path(@employee, experience.id), method: :delete, data: {confirm: "Are You sure?"}, :class=>"btn btn-danger pull-right" %></li>                                         
                                            </ul>
                                           </td>
                                         <% end %>
												                </tr>
											                </tbody>
											                <% end %>
									                </table>  
									            
									                             
                                   
                                </div>
                                <!-- /.col-lg-12 (nested) -->
                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>
