  <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Experiance Details</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Basic Form Elements
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">
                                   <% list.each do |experience| %>
                                        
                                        <div class="form-group">
                                            <label style="font-weight:normal;">Company Name:</label>
                                            <label><%= experience.previous_company %></label>                                 
                                        </div>
                                        
                                        <div class="form-group">
                                            <label style="font-weight:normal;">Designation:</label>
                                            <label><%= experience.last_designation %></label>
                                        </div>
                                        
                                        <div class="form-group">
                                            <label style="font-weight:normal;">From:</label>
                                            <label><%= experience.from_date %> </label>                                         
                                        </div>
                                        
                                        <div class="form-group">
                                            <label style="font-weight:normal;">To:</label>
                                           <label><%= experience.to_date %></label>                                            
                                        </div>
                                         <ul class="navigation-btns" style="list-style: none;display:inline;">
                                        <li><%= link_to 'edit', edit_employee_experience_path(@employee.id, experience.id), :class => "btn btn-success pull-left" %></li>

                                        </ul>
                                        <hr>
                                   <% end %>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                                <div class="col-lg-6">
                                    <h1></h1>
                                    <form role="form">
                                    <!--    <fieldset disabled> -->
                        
                                           
              
                                    </form>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>
