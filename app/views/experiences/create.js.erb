<% if @form_type == "Save" %>
 $(".experince-index").html("<%=j render  'experiences/list', :list => @list %>");
 $(".new-experince").hide(); 
<% else %>
  $(".experince-index").html("<%=j render  'experiences/list', :list => @list %>");
  $(".new-experince").html("<%=j render  'experiences/experience_form' %>");
<% end %>
