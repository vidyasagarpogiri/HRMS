class EmailEttiquittesController < ApplicationController


  def new
    @email = EmailEttiquitie.new
  end
  

  def create
    #raise params.inspect
    @email = EmailEttiquitie.create(params_email)
    redirect_to @email
  end

  def show
    @email= EmailEttiquitie.find(params[:id])
  end
  
  def edit
  end
  
  
  private
   
  def params_email
    params.require(:email).permit(:employee_id, :ettiquite)
  end
end
